{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" },
        { "key": "X-Vercel-Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "build": {
    "env": {
      "VERCEL_FORCE_NO_CACHE": "1"
    }
  }
}
